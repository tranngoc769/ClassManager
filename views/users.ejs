                        <div class="col-xl-12 col-lg-12 filter-bar invoice-list">
                            <div class="row">
                                <% for(var i = 0; i < users.length; i++) { %>
                                    <div name="usercard" class="col-md-3 col-sm-4">
                                        <div class="card card-border-c-blue">
                                            <div class="card-body">
                                                <div>
                                                    <h5 class="d-inline-block m-b-10 "><%=users[i].username %></h5>
                                                </div>
                                                <form>
                                                    <div class="form-group ">
                                                        <label  style="width: 100%;" class="h6 f-12 text-uppercase">Tên tài khoản</label>
                                                        <input readonly disabled style="width: 100%;" class="form-control form-control-sm" target_name="<%=users[i].username %>" type="text" value="<%=users[i].username %>" name="username">
                                                        <label style="width: 100%;" class="h6 f-12 text-uppercase">Mật khẩu</label>
                                                        <input target_name="<%=users[i].username %>" style="width: 100%;" class="form-control form-control-sm"  type="password" value="" name="password">
                                                        <label style="width: 100%;" class="h6 f-12 text-uppercase">Loại tài khoản</label>
                                                        <select name="role" target_name="<%=users[i].username %>" class="form-control form-control-sm">
                                                            <option value="1" <% if(users[i].role==1){ %> selected <% } %>>Admin</option>
                                                            <option value="2" <% if(users[i].role==2){ %> selected <% } %>>Thành viên</option>
                                                        </select>
                                                    </div>
                                                    <div class="form-group">
                                                        <button style="width: 49%;" target_name="<%=users[i].username %>" name="update" type="button" class="btn btn-success"><i class="feather mr-2 icon-check-circle"></i>Cập nhật</button>
                                                        <button style="width: 49%;" target_name="<%=users[i].username %>" name="delete" type="button" class="btn btn-danger"><i class="feather mr-2 icon-delete"></i>Xóa</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
